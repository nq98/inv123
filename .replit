modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_gemini:1.0.0", "web_scraper:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask API Server"

[[workflows.workflow]]
name = "Flask API Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 37409
externalPort = 80

[[ports]]
localPort = 37455
externalPort = 3002

[[ports]]
localPort = 41885
externalPort = 3003

[[ports]]
localPort = 43997
externalPort = 3001

[[ports]]
localPort = 44659
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["bash", "start.sh"]
